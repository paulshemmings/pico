<?xml version="1.0" encoding="utf-8"?>	 
<RelativeLayout
	 xmlns:android="http://schemas.android.com/apk/res/android"
	 android:orientation="vertical"
	 android:layout_width="fill_parent"
	 android:layout_height="fill_parent">
	 
	<include
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_alignParentTop="true"
	    android:layout_marginLeft="101dp"
	    layout="@layout/lib_template_top_stub" 
	    android:layout_above="@+id/lib_map_template_map_container"
	    />    

    <LinearLayout
        android:id="@+id/lib_map_template_map_container"
    	android:orientation="vertical" 
	    android:layout_width="fill_parent"
	    android:layout_height="match_parent"
    	>    
		 <com.google.android.maps.MapView
		  android:id="@+id/lib_map_template_map_view"
		  android:layout_width="fill_parent"
		  android:layout_height="fill_parent"
		  android:clickable="true"
		  android:enabled="true"
		  android:apiKey="0HuVWNM4stRgX5Yp5zvn68mOaj9sPkuzV0dFgDQ" 
		  />
 	</LinearLayout>
 	
	<include
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_alignParentBottom="true"
	    android:layout_below="@id/lib_map_template_map_container"
	    android:layout_marginLeft="101dp"
	    layout="@layout/lib_template_bottom_stub" 
	    />    
           
</RelativeLayout>
